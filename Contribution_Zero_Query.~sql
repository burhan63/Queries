-- CONTRIBUTION 0 --
SELECT * FROM INS_ASSORTED IA 
WHERE IA.ASSORTED_STRING LIKE '%24595-03-2024%' FOR UPDATE;


-- IF STATUS FLAG A THEN ANNUAL RATE OR IF STATUS FLAG R THEN PUT PREVIOUS RATE INTO ANNUAL RATE
SELECT *  FROM INS_HEALTH_EMPLOYEE_INFO IHEI
WHERE IHEI.ASSORTED_CODE = '050000728768'
AND (IHEI.EMP_CODE = IHEI.ADDITION_EMP_CODE OR IHEI.EMP_CODE = IHEI.CHANGE_EMP_CODE)
AND NVL(IHEI.RELATION, 'E') ='E'
FOR UPDATE
--WHERE IHEI.HEALTH_ID = '678965' FOR UPDATE;


-- 0 IN TABLE --
SELECT * FROM INS_HEALTH_EMP_RATE_TABLE IHI
--SET IHI.ANNUAL_RATE = '0'
WHERE IHI.EMP_CODE IN(
'51577491',
'51577494',
'51577497',
'51577499',
'51577501',
'51577481',
'51577484',
'51577487',
'51577489'
)
FOR UPDATE;

