-- HEALTH UNPOSTING METHOD --
SELECT * FROM INS_ASSORTED IA 
WHERE IA.ASSORTED_STRING LIKE '%24355-03-2024%'
AND IA.POLICY_TYPE_CODE = '76';


SELECT * FROM INS_ASSORTED I 
WHERE I.GENERATE_AGAINST = '050000724529';


SELECT * FROM INS_PREMIUM_DET PD 
WHERE PD.ASSORTED_CODE = '050000724529';


SELECT * FROM INS_CLAIM IC 
WHERE IC.INSURANCE_TYPE = '050000724529';

--IF ENDORSEMENT THEN ERASE INSTALLMENT --
SELECT * FROM INS_INSTALLMENT_MASTER IIM
WHERE IIM.ASSORTED_CODE = '050000724529' FOR UPDATE;

SELECT * FROM INS_INSTALLMENT_DETAIL IID 
WHERE IID.INSTALLMENT_MASTER_ID = '41997' FOR UPDATE;
