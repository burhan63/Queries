SELECT * FROM INS_ASSORTED IA 
--WHERE IA.CLIENT_CODE = '08801808'
WHERE IA.ASSORTED_STRING LIKE '%3129-03-2024%' --OR IA.ASSORTED_STRING LIKE '%3035-01-2024%'
AND TRUNC(IA.ENT_DATE) <= TRUNC(SYSDATE)
AND IA.DOCUMENT_CODE = '04'
AND IA.POLICY_TYPE_CODE = '76';

--CHECK PARTTAKER NAME --
SELECT * FROM INS_PARTTAKER IP 
WHERE IP.PARTTAKER_CODE = '08802412'
AND IP.CATEGORY_PARTTAKER_CODE = '08'

SELECT * FROM INS_CUBIC_CAPACITIES IPC

SELECT V.VEHICLE_CODE,IM.MAKE_NAME,V.VEHICLE_NAME ,IPX.CUBIC_HORSE_TITLE,VCZ.VEHICLE_CLASSIFICATION_CODE,
VCZ.CLASSIFICATION_NAME
FROM INS_CUBIC_CAPACITIES IPX, MT_VEHICLES V, MT_MAKES IM, MT_VEHICLES_CLASSIFICATIONS VCZ
WHERE V.CUBIC_HORSE_CODE = IPX.CUBIC_HORSE_CODE
AND V.MAKE_CODE = IM.MAKE_CODE
AND V.VEHICLE_CLASSIFICATION_CODE = VCZ.VEHICLE_CLASSIFICATION_CODE
--ORDER BY TO_NUMBER(VD.VEHICLE_SEQ_NO) ASC

SELECT * FROM MT_VEHICLES_CLASSIFICATIONS


select * from ins_vessel iv
--where iv.vessel_name like '%X-PRESS PHOENIX%'
for update
  
