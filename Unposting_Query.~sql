SELECT * FROM INS_ASSORTED_GEN_CONDITIONS IGC
WHERE IGC.ASSORTED_CODE = '040000178547' FOR UPDATE

SELECT MAX(FUNC_ASSORTED_STRING(K.ASSORTED_CODE)POLICY),K.* FROM INS_ASSORTED K, INS_POLICY_TYPES P, INS_POLICY_CLASS L
WHERE K.POLICY_TYPE_CODE = P.POLICY_TYPE_CODE
AND P.CLASS_CODE = L.CLASS_CODE
AND K.POLICY_TYPE_CODE <> 77
AND K.IS_VALID = 'Y'
AND L.CLASS_CODE = '02'
AND TRUNC(K.ENT_DATE) BETWEEN '27-DEC-2023' AND '28-DEC-2023'
AND K.DOCUMENT_CODE = '04'
ORDER BY 1 ASC

