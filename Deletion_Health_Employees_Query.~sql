select * from ins_claim ic, ins_claim_status ics
where ic.claim_status_id = ics.claim_status_id
and ics.claim_status_id = '3'
and trunc(ent_date) between '21-Mar-2024'and '25-Mar-2024'
and ic.claim_string like '%130111%'

