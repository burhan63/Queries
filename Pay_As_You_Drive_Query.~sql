-- GET MAX PAY_ID --
SELECT MAX(MP.PAY_ID) FROM MT_PAY_DETAILS MP;


-- CHECK DATA --
SELECT func_get_amount_by_type(IA.ASSORTED_CODE,'GROSS')GROSS,
func_get_amount_by_type(IA.ASSORTED_CODE,'NET')NET, IA.PAYD_RATE,ia.remarks,IA.* FROM INS_ASSORTED IA 
WHERE IA.ASSORTED_CODE = '040000150752' OR IA.POLICY_CODE = '040000150752'; for update


