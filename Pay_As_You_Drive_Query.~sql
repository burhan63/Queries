-- PAYD --
-- CHECK CLAIM --
SELECT IC.CLAIM_STATUS_ID,IC.* FROM INS_CLAIM IC 
WHERE IC.INSURANCE_TYPE = '040000148728';


-- PAYD GET DETAILS --
SELECT * FROM MT_PAY_DETAILS TMP
WHERE TMP.VEHICLE_DETAIL_CODE = '1670374'
--WHERE TMP.ASSORTED_CODE = '040000139024'
AND TRUNC(TMP.BILLING_START_FROM) = '12-Mar-2024'
--AND TRUNC(TMP.ENT_DATE) = '23-Feb-2024'        
ORDER BY TMP.RUNNING_DATE DESC 
FOR UPDATE;

