VD.ASSORTED_CODE,vd.insured_value FROM MT_VEHICLES_DETAILS VD
--WHERE VD.Chassis_No LIKE '%ZRE172R7006625%'
--ORDER BY VD.ASSORTED_CODE
WHERE VD.VEHICLE_DETAIL_CODE = '1845915' 
FOR UPDATE

